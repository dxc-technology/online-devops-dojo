{
  "name": "probot",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "No user",
  "license": "ISC",
  "repository": "https://github.com//probot.git",
  "homepage": "https://github.com//probot",
  "bugs": "https://github.com//probot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
